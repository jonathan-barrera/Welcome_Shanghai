<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.welcomeshanghaiapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WhatToSeeActivity"
            android:label="What To See"
            android:parentActivityName=".MainActivity"
            />
        <activity android:name=".WhatToEatActivity"
            android:label="What To Eat"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".WhenToGoActivity"
            android:label="When To Go"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".UsefulPhrasesActivity"
            android:label="Useful Phrases"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>